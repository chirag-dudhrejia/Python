    "str = input(\"Enter String Here : \")\n",
    "\n",
    "upper = 0\n",
    "lower = 0\n",
    "\n",
    "for i in str:\n",
    "    if i.isupper():\n",
    "        upper+=1\n",
    "    elif i.islower():\n",
    "        lower+=1\n",
    "\n",
    "print(\"Upper :\", upper)\n",
    "print(\"lower :\", lower)"

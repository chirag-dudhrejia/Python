   "source": [
    "import math\n",
    "number = int(input(\"Enter number to check primality : \"))\n",
    "flag = 1\n",
    "for i in range(2,int(math.sqrt(number))+1):\n",
    "    if number%i == 0:\n",
    "        print(number,\"is not prime number\")\n",
    "        flag = 0\n",
    "        break\n",
    "    \n",
    "if flag == 1:\n",
    "    print(number,\"is prime number\")"
   ]

   "source": [
    "def palindrome(tpl):\n",
    "    tpl_copy = tpl[::-1]\n",
    "    \n",
    "    if tpl == tpl_copy:\n",
    "        return \"Yes!\"\n",
    "    else:\n",
    "        return \"No!\""
   ]
  },

     "text": [
      "Enter Comma Saperated words : 1,2,3,4,3,2,1\n",
      "Yes!\n"
     ]
    }
   ],
   "source": [
    "tpl = tuple(input(\"Enter Comma Saperated words : \").split(\",\"))\n",
    "\n",
    "print(palindrome(tpl))"
